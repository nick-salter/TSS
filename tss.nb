 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["C", "TI"], "\[Prime]"], TraditionalForm], "errors" -> {}, 
     "input" -> "C'", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d63e5411-b343-4320-b6fd-d1d5b32a0678"],
